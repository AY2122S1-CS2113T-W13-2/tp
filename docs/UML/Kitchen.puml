@startuml
!include Style.puml
skinparam ArrowColor LOGIC_BLUE_5
skinparam classBorderColor LOGIC_BLUE_4
skinparam classBackgroundColor LOGIC_BLUE_1

rectangle "Util" {
    enum "<<Enumeration>> \n Difficulty" {
        NONE
        EASY
        MEDIUM
        HARD
    }

    class Tag
}

rectangle "Kitchen" {
    class Cookbook
    class Recipe

    Cookbook --> "<<Enumeration>> \n Difficulty"
    Cookbook *- Recipe
    Cookbook *--- Tag
}
@enduml