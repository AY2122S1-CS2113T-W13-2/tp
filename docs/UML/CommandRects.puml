@startuml
!include Style.puml
skinparam ArrowColor LOGIC_GREEN_4

rectangle "Command" {
    rectangle "{abstract}\nCommand" LOGIC_PINK_1 {
    }

    rectangle AddCommand LOGIC_GREEN_1 {
    }

    rectangle DeleteCommand LOGIC_GREEN_1 {
    }

    rectangle CheckCommand LOGIC_GREEN_1 {
    }

    rectangle ListRecipesCommand LOGIC_GREEN_1 {
    }

    rectangle HelpCommand LOGIC_GREEN_1 {
    }

    rectangle NullCommand LOGIC_GREEN_1 {
    }

    rectangle FindCaloriesCommand LOGIC_BLUE_1 {
    }

    rectangle FindDifficultyCommand LOGIC_BLUE_1 {
    }

    rectangle FindIngredientsCommand LOGIC_BLUE_1 {
    }

    rectangle FindPriceCommand LOGIC_BLUE_1 {
    }

    rectangle FindTagsCommand LOGIC_BLUE_1 {
    }

    rectangle SetCaloriesCommand LOGIC_RED_1 {
    }

    rectangle SetDifficultyCommand LOGIC_RED_1 {
    }

    rectangle SetPriceCommand LOGIC_RED_1 {
    }


    "{abstract}\nCommand" <|-down- "BasicCommands"
    "{abstract}\nCommand" <|-left- "FindCommands"
    "{abstract}\nCommand" <|-right- "SetCommands"
    "BasicCommands" <|-- AddCommand
    "BasicCommands" <|-- DeleteCommand
    "BasicCommands" <|--- CheckCommand
    "BasicCommands" <|--- ListRecipesCommand
    "BasicCommands" <|--- HelpCommand
    "BasicCommands" <|-- NullCommand
    "FindCommands" <|-left- FindCaloriesCommand
    "FindCommands" <|-- FindDifficultyCommand
    "FindCommands" <|-- FindIngredientsCommand
    "FindCommands" <|--- FindPriceCommand
    "FindCommands" <|--- FindTagsCommand
    "SetCommands" <|-- SetCaloriesCommand
    "SetCommands" <|-- SetDifficultyCommand
    "SetCommands" <|- SetPriceCommand
}
@enduml